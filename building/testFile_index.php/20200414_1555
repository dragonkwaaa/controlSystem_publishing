<script>

// :: 전체시설물, 시설물관리, 기기관리 탭 선택시 색상 변경 스크립트.
$(document).on('click', 'a.tabBtn', function(){
	$('.tabBox a.tabBtn').removeClass('activated');
	$(this).addClass('activated');
});


// :: 테이블 페이징 스크립트.
$(document).on('click', '.pagingBtn', function(){
	$('.pagingBox .pagingBtn').removeClass('activated');
	$(this).addClass('activated');
});

</script>


